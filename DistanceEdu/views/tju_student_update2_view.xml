<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
    <!-- explicit list view definition -->
    <!-- 报名点1 -->
    <record model="ir.ui.view" id="view_from_jm_tju_student_update2">
      <field name="name">天大学籍学生导入form视图</field>
      <field name="model">jm.tju.student.update2</field>
      <field name="arch" type="xml">
          <form string = "窗体名称">
            <sheet>
                  <group string="学生信息">
                    <group>
                      <field name="card_type"/>
                      <field name="idcard"/>
                      <field name="name"/>
                      <field name="nation"/>
                      <field name="sex"/>
                      <field name="native_place"/>
                      <field name="birth"/>
                    </group>
                    <group>
                      <field name="phone"/>
                      <field name="qq"/>
                      <field name="wchat"/>
                      <field name="address"/>
                      <field name="email"/>
                      <field name="job"/>
                      <field name="work_unit"/>
                    </group>

                  </group>
                  <group string="最高学历">
                    <group>
                      <field name="highest_degree"/>
                      <field name="graduate_time"/>
                      <field name="graduate_school"/>
                    </group>
                    <group>
                      <field name="graduate_code"/>
                      <field name="graduate_major"/>
                      <field name="graduate_cer_code"/>
                    </group>

                  </group>

                  <group string="报名信息">
                    <group >
                      <field name = "batch" />
                      <field name = "level" />
                      <field name = "study_center" />
                      <field name = "major" />
                      <field name = "school_date" required = '0'/>
                      <field name = "reg_time" />
                      <field name = "reg_code" />


                    </group>

                    <group string="">
                      <field name = "is_change" required = '0'/>
                      <field name = "warning" required = '0'/>
                      <field name = "college_remark" required = '0'/>
                      <field name = "inputer" required = '0'/>
                      <field name = "beschool" required = '0'/>
                      <field name = "firstcon" required = '0'/>

                    </group>
                  </group>
            </sheet>
          </form>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.ui.view" id="view_tree_jm_tju_student_update2">
      <field name="name">天大学籍学生导入tree视图</field>
      <field name="model">jm.tju.student.update2</field>
      <field name="priority">1</field>
      <field name="arch" type = "xml">
        <tree string = "">
            <field name = "name"/>
            <field name = "idcard" />
            <field name = "sex"/>
            <field name = "native_place"/>
            <field name = "batch"/>
            <field name = "level"/>
            <field name = "study_center"/>
            <field name = "major"/>
            <field name = "school_date"/>
            <field name = "state"/>
        </tree>
      </field>
    </record>


    <record model="ir.actions.act_window" id="action_jm_tju_student_update2">
      <field name="name">天大学籍学生导入</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">jm.tju.student.update2</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
      <field name="help" type = "xml">
        <p class = "oe.view.nocontent_create">
            1.点击左上方的导入按钮  选择要导入的csv文件导入
          <p>
            2.勾选要导入的学生信息  点开动作菜单  点击导入勾选的学生
          </p>
          <p>
            3.导入成功后  将已导入的学生勾选  删除
          </p>
        </p>
      </field>
    </record>


