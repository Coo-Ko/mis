<?xml version="1.0" encoding="UTF-8"?>
<openerp>
  <data>
    <record model="ir.ui.view" id="view_form_jm_tju_student_charge">
      <field name="name">天大学生收费登记form视图</field>
      <field name="model">jm.tju.student</field>
      <field name="priority">1</field>
      <field name="arch" type="xml">
          <form create="false" delete="false">
            <header>
              <button name="workflow_to_confirm" states="init,not_approve" string="提交审核" type="workflow" class="oe_highlight" />
              <button name="workflow_approve" states="confirm" string="审核通过" type="workflow" class="oe_highlight"/>
              <button name="workflow_not_approve" states="confirm" string="审核不通过" type="workflow" class="oe_highlight"/>
              <field name="state" widget="statusbar" statusbar_visible="init,confirm"/>
            </header>
            <sheet>
              <group string="学生信息">
                <group>
                  <field name="student" required="1" readonly="1"/>
                  <field name="name_show"/>

                </group>
                <group>
                  <field name="sex_show"/>
                  <field name="birth_show"/>
                </group>
              </group>
              <group string="报名信息">
                <group >
                  <field name = "batch" readonly="1"/>
                  <field name = "level" readonly="1"/>
                  <field name = "study_center" readonly="1"/>
                  <field name = "major" readonly="1"/>
                </group>
                <group string="">
                  <field name = "study_code" readonly="1"/>
                  <field name = "inputer" readonly="1"/>
                  <field name = "inputer_dpt" readonly="1"/>
                  <field name = "beschool" readonly="1"/>
                </group>
              </group>
              <group string="收费信息">
                <group >
                  <field name = "invoice_money"/>
                  <field name = "invoice"/>
                </group>
                <group string="">
                  <field name = "up_per" readonly="1"/>
                </group>
              </group>

              <notebook>

                <page string="收费明细">
                    <field name="charge_details">
                      <tree string="list" editable="bottom">
                        <field name="project"/>
                        <field name="fee"/>
                        <field name="way"/>
                        <field name="account"/>
                        <field name="charge_time"/>
                        <field name="confirm_state" readonly="1"/>
                        <field name="remarks"/>
                        <field name="deletable" invisible="1"/>
                        <field name="readonly" invisible="1"/>
                      </tree>
                    </field>
                </page>
                <page string="上缴明细">
                    <field name="up_school_detail" readonly="1">
                      <tree string="list" editable="bottom">
                        <field name="project" />
                        <field name="fee" />
                        <field name="way"/>
                        <field name="account" />
                        <field name="charge_time"/>
                        <field name="remarks"/>
                      </tree>
                    </field>
                </page>
                <page string="收费情况">
                    <field name="fee_line" options="{'no_create':'True','no_open': True}" readonly="1">
                      <tree string="list" editable="bottom">
                        <field name="project"/>
                        <field name="fee"/>
                        <field name="fee_charge"/>
                        <field name="fee_charged"/>
                        <field name="up_school_fee"/>
                        <field name="deletable" invisible='1'/>
                      </tree>
                    </field>
                </page>
              </notebook>
            </sheet>
          </form>
      </field>
    </record>

    <!--tree-->
    <record model="ir.ui.view" id="view_tree_jm_tju_student_charge">
      <field name="name">天大学生收费登记tree视图</field>
      <field name="model">jm.tju.student</field>
      <field name="priority">1</field>
      <field name="arch" type = "xml">
        <tree create="false">
            <field name = "name_show"/>
            <field name = "batch" />
            <field name = "level"/>
            <field name = "study_center"/>
            <field name = "beschool"/>
            <field name = "inputer_dpt"/>
        </tree>
      </field>
    </record>

    <!--高级搜索-->
    <record id="view_jm_tju_student_charge_search" model="ir.ui.view">
        <field name="name">天大学生收费登记搜索</field>
        <field name="model">jm.tju.student</field>
        <field name="arch" type="xml">
            <search string="students">
                <field name = "name_show"/>
                <field name = "student"/>
                <field name = "inputer_dpt"/>
                <field name = "batch"/>
            </search>
        </field>
    </record>

    <!--action-->
    <record model="ir.actions.act_window" id="action_jm_tju_student_charge">
      <field name="name">天大学生收费登记</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">jm.tju.student</field>
      <field name="domain">[('state', 'in', ['init', 'not_approve'])]</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
      <field name="search_view_id" ref="view_jm_tju_student_charge_search"/>
    </record>

    <record id="action_tju_student_tree_charge" model="ir.actions.act_window.view">
        <field eval="1" name="sequence"/>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="view_tree_jm_tju_student_charge"/>
        <field name="act_window_id" ref="action_jm_tju_student_charge"/>
    </record>

    <record id="action_tju_student_form_charge" model="ir.actions.act_window.view">
        <field eval="2" name="sequence"/>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_form_jm_tju_student_charge"/>
        <field name="act_window_id" ref="action_jm_tju_student_charge"/>
    </record>



    <record model="ir.actions.act_window" id="action_jm_tju_student_charge_confirm">
